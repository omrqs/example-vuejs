{
  "name": "example-vuejs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --fix --port=8080",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:spec": "jest -c jest.config.spec.json --forceExit --detectOpenHandles",
    "test:unit": "jest -c jest.config.unit.json --forceExit --detectOpenHandles",
    "test:coverage": "jest --coverage --forceExit --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "vue-cli-service lint",
      "pre-push": "vue-cli-service build --mode development"
    }
  },
  "dependencies": {
    "@mdi/font": "^5.3.45",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "lru-cache": "^5.1.1",
    "minify-css-string": "^1.0.0",
    "moment": "^2.26.0",
    "register-service-worker": "^1.6.2",
    "v-mask": "^2.2.1",
    "vue": "^2.6.11",
    "vue-cookies": "^1.7.0",
    "vue-i18n": "^8.18.1",
    "vue-router": "^3.3.2",
    "vuetify": "^2.2.31",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-plugin-pwa": "~4.2.0",
    "@vue/cli-plugin-router": "~4.2.0",
    "@vue/cli-plugin-vuex": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.1.2",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^1.19.1",
    "sass": "^1.26.7",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.4",
    "vuex-router-sync": "^5.0.0"
  }
}
